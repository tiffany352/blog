---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Tiffany's Blog</title>
 <link href="http://lymiahugs.com/~tiffany/blog/atom.xml" rel="self"/>
 <link href="http://lymiahugs.com/~tiffany/blog"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://lymiahugs.com/~tiffany/blog/</id>
 <author>
   <name>Tiffany Bennett</name>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://lymiahugs.com/~tiffany/blog/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://lymiahugs.com/~tiffany/blog/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
